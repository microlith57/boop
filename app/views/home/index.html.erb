<h1>Home#index</h1>
<p>Find me in app/views/home/index.html.erb</p>

<%= form_tag controller: "home", action: "issue" do %>
  <%= label_tag(:issuer, "Issuer Barcode") %>
  <%= text_field_tag(:issuer) %>
  <%= label_tag(:device, "Device Barcode") %>
  <%= text_field_tag(:device) %>
  <%= submit_tag("Issue") %>
<% end %>

<%= form_tag controller: "home", action: "return" do %>
  <%= label_tag(:device, "Device Barcode") %>
  <%= text_field_tag(:device) %>
  <%= submit_tag("Return") %>
<% end %>
